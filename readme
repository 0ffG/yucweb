// user ile giriş
 localStorage.setItem('user', JSON.stringify({ role: 'donor' }));


// ADMİN İLE GİRİŞ
localStorage.setItem('user', JSON.stringify({ role: 'admin' }));


// ÇIKIŞ
localStorage.removeItem('user');
